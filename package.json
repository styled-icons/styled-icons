{
  "name": "styled-icons",
  "version": "1.2.0",
  "description": "Font Awesome, Material Icons, and Octicons available as Styled Components",
  "author": "Jacob Gillespie <jacobwgillespie@gmail.com>",
  "homepage": "https://github.com/jacobwgillespie/styled-icons",
  "repository": "git://github.com/jacobwgillespie/styled-icons.git",
  "license": "MIT",
  "main": "index.js",
  "module": "index.js",
  "jsnext:main": "index.js",
  "typings": "./index.d.ts",
  "sideEffects": false,
  "files": [
    "/fa-brands",
    "/fa-regular",
    "/fa-solid",
    "/material",
    "/octicons",
    "/CHANGELOG.md",
    "/index.d.ts",
    "/index.es5.js",
    "/index.js",
    "/LICENSE",
    "/package.json",
    "/README.md"
  ],
  "keywords": [
    "styled-components",
    "icons",
    "font-awesome",
    "material-ui",
    "material",
    "octicons",
    "svg"
  ],
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "scripts": {
    "build": "node generate/generate.js",
    "build-website": "gatsby build",
    "clean": "rm -rf build fa-brands fa-regular fa-solid material octicons public manifest.json index.d.ts index.es5.js index.js",
    "dev": "gatsby develop",
    "serve": "serve public"
  },
  "peerDependencies": {
    "react": "*",
    "styled-components": ">=3 <4"
  },
  "devDependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-brands": "^5.0.10",
    "@fortawesome/fontawesome-free-regular": "^5.0.10",
    "@fortawesome/fontawesome-free-solid": "^5.0.10",
    "@types/node": "^9.6.5",
    "@types/react": "^16.3.11",
    "babel-plugin-styled-components": "^1.5.1",
    "copy-to-clipboard": "^3.0.8",
    "execa": "^0.10.0",
    "fast-case": "^1.1.0",
    "fast-glob": "^2.2.0",
    "fs-extra": "^5.0.0",
    "gatsby": "^1.9.247",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-netlify": "^1.0.19",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-plugin-styled-components": "^2.0.11",
    "h2x-core": "^0.1.9",
    "h2x-plugin-jsx": "^0.1.9",
    "js-search": "^1.4.2",
    "material-design-icons": "^3.0.1",
    "number-to-words": "^1.2.3",
    "octicons": "^7.2.0",
    "ora": "^2.0.0",
    "prettier": "^1.12.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-virtualized": "^9.18.5",
    "serve": "^6.5.5",
    "styled-components": "^3.2.5",
    "svgo": "^1.0.5",
    "typescript": "^2.8.1"
  }
}
